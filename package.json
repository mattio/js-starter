    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "numeral": "^2.0.3",
    "open": "0.0.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "0.0.5"
